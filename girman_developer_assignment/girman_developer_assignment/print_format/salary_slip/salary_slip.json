 "html": "<div class=\"letter-head\">{{ letter_head }}</div>\n <hr>\n  <div class=\"print-heading\"><h2>Salary slip<br><small class=\"sub-heading\">{{ doc.name }}</small></h2></div>\n<div>\n    <span> <img src=\"/files/WHITE copy 2@4x-8.png\" class=\"watermark\"></span>\n    <table width=\"100%\">\n    <tr>\n        <td><b>Employee Name:</b>&nbsp;{{doc.employee_name}}</td>\n        <td ><b>Start Date:</b>&nbsp;{{doc.get_formatted(\"start_date\")}}</td>\n    </tr>\n    <tr>\n        <td><b>Employee:</b>&nbsp;{{doc.employee}}</td>\n        <td><b>End Date:</b>&nbsp;{{doc.get_formatted(\"end_date\")}}</td>\n    </tr>\n    <tr>\n        <td><b>Company:</b>&nbsp;{{doc.company}}</td>\n        <td><b>Working Days:</b>&nbsp;{{doc.total_working_days}}</td>\n    </tr>\n    <tr>\n        <td></td>\n        <td><b>Leave Without Pay:</b>&nbsp;{{doc.leave_without_pay}}</td>\n    </tr>\n    <tr>\n        <td></td>\n        <td><b>Payment Days:</b>&nbsp;{{doc.payment_days}}</td>\n    </tr>\n</table>\n<br>\n    <div>\n    <table style=\"border:1px solid #d1d8dd; width:100%;\">\n        <tr>\n            <th  class=\"th1\">S.No</th>\n            <th   class=\"th1\">Component</th>\n            <th   class=\"th1\">Amount</th>\n        </tr>\n         {% for i in doc.earnings%}\n        <tr>\n            <td style=\"border:1px solid #d1d8dd;\">{{i.get_formatted(\"idx\")}}</td>\n            <td style=\"border:1px solid #d1d8dd;\">{{i.get_formatted(\"salary_component\")}}</td>\n            <td style=\"border:1px solid #d1d8dd;\">{{i.get_formatted(\"amount\")}}</td></tr>\n           {% endfor %}\n    </table>\n    </div>\n    <br><br>\n   \n    <div class=\"col-xs-8\">\n    <table>\n            <tr>\n                <td class=\"col-xs-4\"><b>Total In Words</b></td>\n                <td class=\"col-xs-9\">:  {{doc.get_formatted(\"total_in_words\")}}</td>\n            </tr>\n    </table>\n    </div>\n    <div class=\"col-xs-4\">\n    <table>\n        <tr>\n            <td class=\"col-xs-4\" align=\"right\"><b>Gross Pay</b></td>\n            <td class=\"col-xs-3\">:{{doc.get_formatted(\"gross_pay\")}}</td>\n        </tr>\n            <tr>\n                <td class=\"col-xs-4\" align=\"right\"><b>Total Deduction</b></td>\n                <td class=\"col-xs-2\">:{{doc.get_formatted(\"total_deduction\")}}</td>\n            </tr>\n            <tr>\n                <td class=\"col-xs-4\" align=\"right\"><b>Net Pay</b></td>\n                <td class=\"col-xs-2\">:{{doc.get_formatted(\"net_pay\")}}</td>\n            </tr>\n            <!--{%- for row in doc.taxes -%}\n            {%- if row.tax_amount -%}-->\n            <!--<tr>\n                <td class=\"col-xs-4\">{{ row.description }}</td>\n                <td class=\"col-xs-2\">:{{ row.get_formatted(\"tax_amount\", doc) }}</td>\n            </tr>\n            {%- endif -%}\n            {%- endfor -%}-->\n            <tr>\n                <td class=\"col-xs-4\" align=\"right\"><b>Round Total</b></td>\n                <td class=\"col-xs-2\">:{{doc.get_formatted(\"rounded_total\")}}</td>\n            </tr>\n    </table>\n    </div>\n    <hr>\n</div>\n{{footer}}",
